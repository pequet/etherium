{
"manifest_version": 2,
    "name": "Etherium>Ethereum",
    "description": "Replaces every instance of the word Etherium with Ethereum",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}